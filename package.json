{
  "name": "libp2p-pstn",
  "version": "0.0.1",
  "description": "Benchmark utility for designing and testing new js-libp2p pubsub strategies",
  "main": "src/network.js",
  "scripts": {
    "start": "DEBUG=pstn:logger* node example.js 2> logs/log.log",
    "start:debug": "DEBUG=pstn:stats* node example.js",
    "test": "mocha -t 60000 test/**/*.spec.js"
  },
  "keywords": [
    "js-ipfs",
    "ipfs",
    "libp2p",
    "pubsub"
  ],
  "author": "Gavin McDermott <gavinmcdermott@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "debug": "2.2.0",
    "libp2p-floodsub": "0.3.0",
    "libp2p-ipfs": "0.14.1",
    "libp2p-pstn-logger": "0.0.1",
    "libp2p-pstn-stats": "0.0.4",
    "libp2p-pstn-topo-ring": "0.0.3",
    "memwatch-next": "0.3.0",
    "multiaddr": "2.0.3",
    "peer-id": "0.7.0",
    "peer-info": "0.7.1",
    "ramda": "0.22.1"
  },
  "devDependencies": {
    "chai": "3.5.0"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gavinmcdermott/js-libp2p-pstn.git"
  },
  "bugs": {
    "url": "https://github.com/gavinmcdermott/js-libp2p-pstn/issues"
  },
  "homepage": "https://github.com/gavinmcdermott/js-libp2p-pstn#readme"
}
