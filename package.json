    "libp2p-pstn-stats": "0.0.4",
    "libp2p-pstn-topo-partialmesh": "0.0.2",
    "libp2p-pstn-topo-ring": "0.0.3",
    "memwatch-next": "0.3.0",
    "multiaddr": "2.0.3",
    "node-uuid": "1.4.7",
